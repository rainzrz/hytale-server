# Token do bot do Discord (obtenha em https://discord.com/developers/applications)
DISCORD_TOKEN=seu_token_aqui

# ID do canal do Discord para notificações (clique direito no canal > Copiar ID)
DISCORD_CHANNEL_ID=seu_canal_id_aqui

# URL da API do Uptime Kuma (use o nome do container Docker)
KUMA_URL=http://uptime-kuma:3001/api/status-page/heartbeat/hytale

# API Key do Uptime Kuma (configure no Kuma: Settings > API Keys)
KUMA_API_KEY=sua_api_key_aqui
